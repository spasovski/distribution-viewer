@import 'lib';

.chart-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  width: 100%;

  .chart {
    height: $chart-preview-height;
    padding-bottom: $lrg-padding;
    position: relative;
    width: $chart-preview-width;
  }
}

.chart-item-wrapper {
  h2 {
    text-align: center;
    font-weight: 100;
    font-size: 16px;
  }
}
